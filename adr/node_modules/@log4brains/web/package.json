{
  "name": "@log4brains/web",
  "version": "1.1.0",
  "description": "Log4brains architecture knowledge base web UI and static site generator",
  "keywords": [
    "log4brains"
  ],
  "author": "Thomas Vaillant <thomvaill@bluebricks.dev>",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/thomvaill/log4brains",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomvaill/log4brains",
    "directory": "packages/web"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "nextjs"
  ],
  "source": "./index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "yarn dev:cli",
    "build": "yarn build:cli && yarn build:nextjs",
    "clean": "yarn clean:cli && yarn clean:nextjs",
    "dev:cli": "microbundle --no-compress --format es,cjs --tsconfig tsconfig.build.json --target node watch",
    "build:cli": "microbundle --no-compress --format es,cjs --tsconfig tsconfig.build.json --target node",
    "clean:cli": "rimraf ./dist",
    "dev:nextjs": "cd nextjs && cross-env LOG4BRAINS_CWD=../.. NODE_OPTIONS=--openssl-legacy-provider next",
    "build:nextjs": "yarn clean:nextjs && cd nextjs && cross-env LOG4BRAINS_PHASE=initial-build NODE_OPTIONS=--openssl-legacy-provider next build",
    "clean:nextjs": "rimraf ./nextjs/.next ./nextjs/.next-export",
    "serve": "serve .log4brains/out",
    "typescript": "tsc --noEmit && cd nextjs && tsc",
    "test": "jest",
    "lint": "eslint . --max-warnings=0",
    "storybook": "cd nextjs && cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@log4brains/cli-common": "1.1.0",
    "@log4brains/core": "1.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@next/bundle-analyzer": "^10.0.1",
    "@types/node": "^15.0.1",
    "@types/react": "17.0.4",
    "babel-plugin-import": "^1.13.1",
    "badge-maker": "^3.3.0",
    "chalk": "^4.1.0",
    "clsx": "^1.1.1",
    "commander": "^6.1.0",
    "copy-text-to-clipboard": "^2.2.0",
    "highlight.js": "^10.4.0",
    "lunr": "^2.3.9",
    "markdown-to-jsx": "^7.0.1",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "next": "10.2.3",
    "open": "^7.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "slugify": "^1.4.5",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/react": "^6.1.9",
    "@types/lunr": "^2.3.3",
    "@types/mkdirp": "^1.0.1",
    "@types/react-test-renderer": "^16.9.3",
    "@types/signale": "^1.4.1",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.34",
    "@types/url-parse": "^1.4.3",
    "babel-jest": "^26.6.0",
    "babel-loader": "^8.1.0",
    "copyfiles": "^2.4.0",
    "del-cli": "^3.0.1",
    "microbundle": "^0.12.4",
    "react-is": "^16.13.1",
    "react-test-renderer": "^17.0.1"
  },
  "gitHead": "536e648995d20cf8c9b43c2d0b95ab25d151ffb5"
}
